<div id="my-or-all-submissions-info" class="content">
    <br />

    <table id="my-or-all-submissions-table" class="table table-striped table-bordered table-hover table-responsive table-condensed assists-data col-xs-12">
      <tr>
        <th class="content-cell content-header col-xs-4" data-id={{id}}>Name</th>
        <th class="content-cell content-header col-xs-5" data-id={{id}}>Challenge</th>
        <th class="content-cell content-header col-xs-1" data-id={{id}}>Pass</th>
        <th class="content-cell content-header col-xs-1" data-id={{id}}>Versions</th>
        <th class="content-cell content-header col-xs-1" data-id={{id}}>Delete</th>
      </tr>
      {{#each submissions}}
        <tr>
          <td class="standings-cell standings-row col-xs-4" data-id={{id}}>{{name}}</td>
          <td class="standings-cell standings-row col-xs-5" data-id={{id}}>{{_challenge}}</td>
          <td class="standings-cell standings-row col-xs-1" data-id={{id}}>{{pass}}</td>
          <td class="standings-cell standings-row col-xs-1" data-id={{id}}>Coming Soon</td>
          <td class="standings-cell standings-row col-xs-1">
            <button class="btn btn-default assist-delete-button admin-only" data-id={{id}}>Coming Soon</button>
          </td>
        </tr>
      {{/each}}
    </table>
</div>
